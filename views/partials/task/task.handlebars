<div class="row">
                <ul>
                  {{#each tasks}}
                  {{#dataValues}}
                  {{#unless task_complete}}
                  {{> taskstatus/taskstatus task_complete=false}}
                  {{/unless}}
                  {{/dataValues}}
                  {{/each}}
                </ul>

              </div>

              <div class="row">
                <h2>Completed Tasks</h2>
              </div>
                <div class="row">
                  <ul>
                    {{#each tasks}}
                    {{#dataValues}}
                    {{#if task_complete}}
                    {{> taskstatus/taskstatus task_complete=true}}
                    {{/if}}
                    {{/dataValues}}
                    {{/each}}
                  </ul>

                </div>